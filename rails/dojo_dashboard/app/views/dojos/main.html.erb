<h1>Listing <%= @count %> Dojos</h1>

<table>
    <tr>
        <th>Coding Dojo Branch</th>
        <th>Steet</th>
        <th>City</th>
        <th>State</th>
        <th>Actions</th>
    </tr>
    <% @dojos.each do |dojo| %>
        <tr>
            <td>CD <%= dojo.branch %></td>
            <td><%= dojo.street %></td>
            <td><%= dojo.city %></td>
            <td><%= dojo.state %></td>
            <td>
                <%= link_to "Show", "/dojos/#{dojo.id}" %>
                <%= link_to "Edit", "/dojos/#{dojo.id}/edit" %>
                <%= link_to 'Destroy', "/dojos/#{dojo.id}",
                method: :delete, data: { confirm: 'Are you sure?' } %></td> 

            </td>
        </tr>
    <% end %>
</table>
    <br>
    
    <%= link_to "New Dojo", :new %>

</table>